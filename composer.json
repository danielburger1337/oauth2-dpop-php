{
    "name": "danielburger1337/oauth2-dpop",
    "type": "library",
    "description": "Create/Verify OAuth2 DPoP tokens.",
    "license": "MIT",
    "homepage": "https://github.com/danielburger1337/oauth2-dpop-php",
    "authors": [
        {
            "name": "Daniel Burger",
            "homepage": "https://github.com/danielburger1337"
        }
    ],
    "keywords": [
        "oauth2",
        "dpop"
    ],
    "require": {
        "php": "^8.4",
        "psr/clock": "^1.0",
        "spomky-labs/base64url": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.91",
        "nyholm/psr7": "^1.8",
        "paragonie/constant_time_encoding": "^3.1",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.5",
        "psr/cache": "^3.0",
        "psr/http-message": "^2.0",
        "spomky-labs/otphp": "^11.3",
        "symfony/clock": "^7.2 || ^8.0",
        "symfony/http-foundation": "^7.2 || ^8.0",
        "web-token/jwt-library": "^3.4.6 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "danielburger1337\\OAuth2\\DPoP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "danielburger1337\\OAuth2\\DPoP\\Tests\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
